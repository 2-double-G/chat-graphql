{
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "esnext",
    "noImplicitAny": true,
    "outDir": "./build/",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noFallthroughCasesInSwitch": true,
    "strictFunctionTypes": false,
    "typeRoots": ["src/types"],
    "lib": ["esnext", "dom", "dom.iterable"],
    "moduleResolution": "node"
  },
  "include": ["src/components/index.tsx", "src/**/*", "../server/psevdoDB/users.js", "../server/psevdoDB/dialogs.js", "../server/psevdoDB/rooms.js", "../server/psevdoDB/currentUser.js"]
}
